{
    "manifest_version": 3,
    "name": "NoDistract",
    "version": "1.0",
    "description": "Filters YouTube categories and channels to reduce distractions.",
    "permissions": ["storage"],
    "host_permissions": ["https://www.youtube.com/*"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/assets/ext-icon.png",
        "48": "/assets/ext-icon.png",
        "128": "/assets/ext-icon.png"
      }
    }
  }